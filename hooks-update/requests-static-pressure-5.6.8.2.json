        ]
      },
      "groupVariables": [
        {
          "uuid": "",
          "label": "inDamperLoop",
          "attrs": {
            "airRef": "{{ Point.Attrs.airRef }}",
            "description": "Damper Loop Status",
            "equipRef": "{{ Point.Attrs.equipRef }}",
            "label": "inDamperLoop"
          }
        },
        {
          "uuid": "",
          "label": "Cooling_SP_Requests",
